<h2><%= t('form.post_import.heading') %></h2>


<%= error_messages_for :post_import %>

<% form_for(:post_import,  :url => admin_post_import_index_path, :method => :post, :html => { :multipart => true }) do |f| %>
<fieldset>
  <%= f.label :data_file, t('form.post_import.new.data_file') %>
  <%= f.file_field :data_file %>
  <%= f.error_message_on :data_file %>
</fieldset>

<p class="form-buttons">
  <%= button t("actions.create") %>
</p>

<% end %>
